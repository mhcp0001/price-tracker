{
  "name": "price-tracker-pwa",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "スーパーマーケット価格追跡PWAアプリ",
  "keywords": [
    "price-tracker",
    "pwa",
    "react",
    "supabase",
    "grocery"
  ],
  "author": "Price Tracker Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:setup": "node scripts/setup-test-db.js",
    "test:cleanup": "node scripts/cleanup-test-db.js",
    "test:all": "npm run test:unit && npm run test:e2e",
    "install:playwright": "playwright install --with-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@mapbox/mapbox-gl-geocoder": "^5.0.0",
    "@rollup/rollup-win32-x64-msvc": "^4.46.2",
    "@supabase/supabase-js": "^2.38.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.292.0",
    "mapbox-gl": "^2.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/mapbox-gl": "^2.7.17",
    "@eslint/js": "^9.0.0",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/mapbox-gl": "^2.7.17",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.0",
    "postcss": "^8.4.31",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.0.0"
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

